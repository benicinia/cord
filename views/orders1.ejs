
<% include usrnv %>


  

  
 
          
          <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
                        <th>Trans. ID</th>
                        <th>Date & Time </th>
                        <th>Remarks</th>
                        <th>Order details</th>
                         <th>Amount</th>
                        <th>Withdrawal</th>
                        <th>Balance</th>
                        
                    </thead>
                   

     <tbody > 
        
        <%
        if(userData.length!=0){
        var i=1;
        userData.forEach(function(data){
        %>
        <tr>
            <td><%=data.trans_id %></td>
            <td><%=data.trans_id %></td>
            <td><%=data.remarks %></td>
            <td><a href="/order?t_id=<%=data.trans_id %>">View</a></td>
            <td><%=data.debit %></td>
            <td><%=data.credit %></td>
            <td><%=data.balance %></td>
            
           
        </tr>
        <%  i++; }) %>
        </tbody>
        <% } else{ %>
            
                <p id="none"> No results found :(</p>
            
        <% } %>
     </table>
            </table>
 <!-- /.container-fluid-->
    <!-- /.content-wrapper-->
    
    
    <!-- Bootstrap core JavaScript-->
    
    <script src="javascripts/vendor/datatables/jquery.dataTables.js"></script>
    <script src="javascripts/vendor/datatables/dataTables.bootstrap4.js"></script>
    <!-- Custom scripts for all pages-->
    <script src="javascripts/js/sb-admin.min.js"></script>
    <!-- Custom scripts for this page-->
    <script src="javascripts/js/sb-admin-datatables.min.js"></script>
  </div>
</body>

</html>
