                        <th><h7 >Withdrawal</h7></th>
                        <th><h7 >Balance</h7></th>
                        
                    </thead>
                   

     <tbody > 
        
        {{#if data}}
        {{#each data}}
        <tr>
            <td>{{this.trans_id}} </td>
            <td>{{this.trans_date}}</td>
            <td>{{this.remarks}}</td>
            <td><a href="/order/{{this.trans_id}}">View</a></td>
            <td>{{this.debit}}</td>
            <td>{{this.credit}}</td>
            <td>{{this.balance}}</td>
            
           
        </tr>
       {{/each}}
        </tbody>
        
        {{else}}
            
                <p id="none"> No results found :(</p>
            {{/if}}
       
